{
  "adb": {
    "type": "stdio",
    "command": "adb",
    "args": [
      "mcp",
      "serve"
    ]
  },
  "aws-docs": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "awslabs.aws-documentation-mcp-server@latest"
    ],
    "env": {}
  },
  "aws-knowledge": {
    "type": "http",
    "url": "https://knowledge-mcp.global.api.aws"
  },
  "context7": {
    "type": "http",
    "url": "https://mcp.context7.com/mcp",
    "headers": {
      "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY:-}"
    }
  }
}
